{% if slug is not defined %}
    {% set slug = 'index' %}
{% endif %}
{% set params = {
    'loggedIn': app.request.get('loggedIn')|default('1'),
    'registered': app.request.get('registered')|default('1'),
} %}
{% set flash = app.request.get('flash')|default('0') %}
{% set flashType = app.request.get('flashType')|default('info') %}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <link rel="dns-prefetch" href="//fonts.googleapis.com" />
    <link rel="dns-prefetch" href="//themes.googleusercontent.com" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>{% filter striptags|trim %}{% block title %}SIUX Prototype{% endblock %}{% endfilter %}</title>

    {% block styles %}
        <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:400,300,700&amp;subset=latin,latin-ext" />
        <link rel="stylesheet" href="{{ app.request.basepath }}/css/main.min.css" />
    {% endblock %}

    <link rel="icon" href="data:image/png;base64," />
</head>
<body class="has-navbar-fixed-top has-footer-sticky">

{% include '_partials/flash_messages.html.twig' %}

{% include '_partials/header.html.twig' %}

<main>
    {% block header %}
        <section class="section section-header">
            <div class="container">
                <h1>
                    {% include '_partials/icon.html.twig' with {'name': 'dashboard', 'class': 'icon-offset icon-vertically-aligned text-muted'} %}
                    Admin
                </h1>
            </div>
        </section>
    {% endblock %}

    <div class="section section-content">
        <div class="container">

            {% block body %}{% endblock %}

        </div>
    </div>
</main>

{% include '_partials/footer.html.twig' %}

{% block scripts %}
    <script src="{{ app.request.basepath }}/js/main.min.js"></script>
{% endblock %}

</body>
</html>
